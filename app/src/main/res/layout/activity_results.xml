<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.osu.dconvapp.ResultsActivity">



    <SeekBar
        android:id="@+id/seekBarID"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1dp"
        android:layout_marginStart="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp" />




    <com.jjoe64.graphview.GraphView
        android:layout_width="336dp"
        android:layout_height="140dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/seekBarID"
        android:id="@+id/grapheb"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="0dp"
        android:layout_height="140dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/grapheb"
        android:id="@+id/graphm"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/graphy"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/graphy" />


    <com.jjoe64.graphview.GraphView
        android:layout_width="0dp"
        android:layout_height="140dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/graphm"
        android:id="@+id/graphy"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp" />

</android.support.constraint.ConstraintLayout>
